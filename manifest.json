{
    "manifest_version": 2,
    "name": "DebugRemover",
    "version": "1.0",
    "description": "Remove all print or debug statements from a webpage JavaScript.",
    "permissions": ["activeTab", "storage", "tabs", "<all_urls>"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      "removeDebugs.js"
    ]
}
